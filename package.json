{
  "name": "microsoft-terminology-search",
  "displayName": "Microsoft Terminology Search",
  "description": "Search terms at Microsoft Terminology Search",
  "version": "0.0.3",
  "publisher": "bugron",
  "engines": {
    "vscode": "^1.31.0"
  },
  "author": {
    "name": "Arsen Melikyan",
    "url": "https://www.github.com/bugron"
  },
  "icon": "resources/icon.png",
  "homepage": "https://www.github.com/bugron/microsoft-terminology-search",
  "repository": {
    "type": "git",
    "url": "https://www.github.com/bugron/microsoft-terminology-search"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.terminologySearch"
  ],
  "main": "./extension.js",
  "contributes": {
    "menus": {
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "extension.terminologySearch",
          "group": "navigation@-1"
        }
      ]
    },
    "commands": [
      {
        "command": "extension.terminologySearch",
        "title": "Search with Microsoft Terminology"
      }
    ],
    "configuration": {
      "title": "Microsoft Terminology Search",
      "type": "object",
      "properties": {
        "terminologySearch.TranslateFrom": {
          "type": "string",
          "default": "en-US",
          "description": "Translate from"
        },
        "terminologySearch.TranslateInto": {
          "type": "string",
          "default": "ru-RU",
          "description": "Translate into"
        },
        "terminologySearch.SearchOperator": {
          "type": "string",
          "default": "Contains",
          "enum": [
            "Exact",
            "Contains",
            "AnyWord"
          ],
          "description": "Match type"
        },
        "terminologySearch.ConfidenceLevel": {
          "type": "number",
          "default": "70",
          "description": "Minimum confidence level"
        }
      }
    }
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test",
    "build": "vsce package"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^10.12.21",
    "eslint": "^7.20.0",
    "typescript": "^3.3.1",
    "vsce": "^1.85.0",
    "vscode": "^1.1.37"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "xml2js": "^0.4.23"
  }
}
